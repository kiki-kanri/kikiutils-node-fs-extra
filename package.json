{
  "name": "@kikiutils/fs-extra",
  "version": "2.3.6",
  "description": "Wraps node:fs, node:fs/promises, and fs-extra functions with try-catch, returning success/failure indicators. Includes additional file and path utilities.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node-fs-extra.git"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "ts-project-builder ./src/index.ts --clean --preserve-modules",
    "bumplog": "changelogen --bump",
    "prepack": "pnpm run build",
    "release": "pnpm run build && changelogen --push --release && npm publish"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.4",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.7.0",
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/node": "^22.7.6",
    "ts-project-builder": "3.3.0"
  },
  "keywords": [
    "copy",
    "create",
    "delete",
    "directory",
    "extra",
    "file",
    "file system",
    "fs",
    "fs-extra",
    "json",
    "mkdir",
    "mkdirp",
    "mkdirs",
    "move",
    "output",
    "promise",
    "read",
    "remove",
    "text",
    "touch",
    "write"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
